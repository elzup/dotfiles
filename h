[32m+[m[32mjs <<EOM[m
[32m+[m
[32m+[m[32mfunction getShortUri(url) {[m
[32m+[m[32m    let uri = 'https://www.googleapis.com/urlshortener/v1/url';[m
[32m+[m[32m    let xhr = new XMLHttpRequest();[m
[32m+[m[32m    xhr.open("post", uri, false);[m
[32m+[m[32m    xhr.setRequestHeader("Content-Type", "application/json");[m
[32m+[m[32m    xhr.send(JSON.stringify({ longUrl: url }));[m
[32m+[m[32m    return JSON.parse(xhr.responseText).id;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mcommands.addUserCommand([m
[32m+[m[32m    ["shorturi"],[m
[32m+[m[32m    "get google short URI this page",[m
[32m+[m[32m    function(args, bang, count){[m
[32m+[m[32m        let url = buffer.URL;[m
[32m+[m[32m        if (args[0]) {url = args[0];}[m
[32m+[m[32m        let result = getShortUri(url);[m
[32m+[m[32m        util.copyToClipboard(result);[m
[32m+[m[32m        liberator.echo('Created shortURL: ' + result + "<--" + url);[m
[32m+[m[32m    }[m
[32m+[m[32m);[m
[32m+[m
[32m+[m
[32m+[m[32mcommands.addUserCommand([m
[32m+[m[32m    ["now"],[m
[32m+[m[32m    "echo timestamp just now",[m
[32m+[m[32m    function(args, bang, count){[m
[32m+[m[32m        var d = new Date();[m
[32m+[m[32m        liberator.echo((d.getYear() + 1900) + '-' + d.getMonth() + '-' +  d.getDay() + ' ' + d.getHours() + ':' + d.getMinutes() + ':' + d.getSeconds());[m
[32m+[m[32m    }[m
[32m+[m[32m);[m
[32m+[m
[32m+[m[32mcommands.addUserCommand([m
[32m+[m[32m    ["yankinfo"],[m
[32m+[m[32m    "get current page info and yank to clipboard",[m
[32m+[m[32m    function(args, bang, count){[m
[32m+[m[32m        let result = getShortUri(buffer.URL);[m
[32m+[m[32m        util.copyToClipboard(result);[m
[32m+[m[32m        liberator.echo("copyed short url");[m
[32m+[m[32m    },[m
[32m+[m[32m    {[m
[32m+[m[32m        subCommands: [[m
[32m+[m[32m            new Command([m
[32m+[m[32m            ["title"],[m
[32m+[m[32m            "page title yank to clipboard",[m
[32m+[m[32m            function() {[m
[32m+[m[32m                util.copyToClipboard(buffer.title);[m
[32m+[m[32m                liberator.echo("yanked title: " + buffer.title);[m
[32m+[m[32m            }),[m
[32m+[m[32m            new Command([m
[32m+[m[32m            ["url"],[m
[32m+[m[32m            "page url yank to clipboard",[m
[32m+[m[32m            function() {[m
[32m+[m[32m                util.copyToClipboard(buffer.URL);[m
[32m+[m[32m                liberator.echo("yanked url: " + buffer.URL);[m
[32m+[m[32m            }),[m
[32m+[m[32m            new Command([m
[32m+[m[32m            ["titleandurl"],[m
[32m+[m[32m            "page title and url yank to clipboard",[m
[32m+[m[32m            function() {[m
[32m+[m[32m                let text = [buffer.title, buffer.URL].join(" ");[m
[32m+[m[32m                util.copyToClipboard(text);[m
[32m+[m[32m                liberator.echo("yanked title and url: " + text);[m
[32m+[m[32m            }),[m
[32m+[m[32m            new Command([m
[32m+[m[32m            ["titleandsurl"],[m
[32m+[m[32m            "page title and shorturl yank to clipboard",[m
[32m+[m[32m            function() {[m
[32m+[m[32m                let text = [buffer.title, getShortUri(buffer.URL)].join(" ");[m
[32m+[m[32m                util.copyToClipboard(text);[m
[32m+[m[32m                liberator.echo("yanked title and short url: " + text);[m
[32m+[m[32m            }),[m
[32m+[m[32m            new Command([m
[32m+[m[32m            ["shorturl"],[m
[32m+[m[32m            "page shorturl yank to clipboard",[m
[32m+[m[32m            function() {[m
[32m+[m[32m                let result = getShortUri(buffer.URL);[m
[32m+[m[32m                util.copyToClipboard(result);[m
[32m+[m[32m                liberator.echo("yanked short url: " + result);[m
[32m+[m[32m            }),[m
[32m+[m[32m        ][m
[32m+[m[32m    }[m
[32m+[m[32m);[m
[32m+[m
[32m+[m[32mcommands.addUserCommand([m
[32m+[m[32m    ["focuss[earchbar]"],[m
[32m+[m[32m    "focus to searchbar",[m
[32m+[m[32m    function() {[m
[32m+[m[32m        if (window.content.window.location.hostname.indexOf("www.google.co.jp") != -1)[m[41m [m
[32m+[m[32m        {[m
[32m+[m[32m            window.content.window.document.getElementsByName("q")[0].focus();[m
[32m+[m[32m            liberator.echo("get focusd");[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m[32m);[m
[32m+[m
[32m+[m[32mEOM[m
[32m+[m[32m"//        echo 'Copied text to clipboard[shorturl]: ' + result;[m
