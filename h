[32m+[m[32m#!/bin/sh[m
[32m+[m[32m#[m
[32m+[m[32m# ~/.xinitrc[m
[32m+[m[32m#[m
[32m+[m[32m# Executed by startx (run your window manager from here)[m
[32m+[m
[32m+[m[32mif [ -d /etc/X11/xinit/xinitrc.d ]; then[m
[32m+[m[32m  for f in /etc/X11/xinit/xinitrc.d/*; do[m
[32m+[m[32m    [ -x "$f" ] && . "$f"[m
[32m+[m[32m  done[m
[32m+[m[32m  unset f[m
[32m+[m[32mfi[m
[32m+[m
[32m+[m[32mxrdb -merge $HOME/.Xresources[m
[32m+[m
[32m+[m[32m# IME[m
[32m+[m[32m#export GTK_IM_MODULE=uim[m
[32m+[m[32m#export QT_IM_MODULE=uim[m
[32m+[m[32m#export XMODIFIERS=@im=uim[m
[32m+[m[32m#uim-xim &[m[41m [m
[32m+[m[32m#uim-toolbar-gtk &[m
