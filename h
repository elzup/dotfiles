[36m@@ -0,0 +1,73 @@[m
[32m+[m[32m/**[m
[32m+[m[32m * ==VimperatorPlugin==[m
[32m+[m[32m * @name              IME Controller Lite[m
[32m+[m[32m * @description       control imput method at into commandline-mode[m
[32m+[m[32m * @description-ja    ÉRÉ}ÉìÉhÉâÉCÉìÉÇÅ[Éhà⁄çséûÇ…IMEÇÃêßå‰ÇçsÇ§[m
[32m+[m[32m * @author            teramako teramako@gmail.com[m
[32m+[m[32m * @namespace         http://d.hatena.ne.jp/teramako/[m
[32m+[m[32m * @maxVersion        0.6pre[m
[32m+[m[32m * @minVersion        0.6pre[m
[32m+[m[32m * ==/VimperatorPlugin==[m
[32m+[m[32m *[m
[32m+[m[32m *  Please set g:ex_ime_mode and g:textarea_ime_mode value.[m
[32m+[m[32m *[m[41m [m
[32m+[m[32m *  g:ex_ime_mode:[m
[32m+[m[32m *   used at into EX mode[m
[32m+[m[32m *[m[41m [m
[32m+[m[32m *  g:textarea_ime_mode:[m
[32m+[m[32m *   used at into TEXTAREA mode from INSERT mode and "noinsertmode" is set.[m
[32m+[m[32m *[m[41m [m
[32m+[m[32m *  ex).[m
[32m+[m[32m *  :let g:ex_ime_mode = "inactive"[m
[32m+[m[32m *  :let g:textarea_ime_mode = "inactive"[m
[32m+[m[32m *[m
[32m+[m[32m *  following values are available:[m
[32m+[m[32m *      "auto"     : No change[m
[32m+[m[32m *      "active"   : Initially IME on[m
[32m+[m[32m *      "inactive" : Initially IME off[m
[32m+[m[32m *      "disabled"  : Disable IME[m
[32m+[m[32m *[m
[32m+[m[32m *  more details: see http://developer.mozilla.org/en/docs/CSS:ime-mode[m
[32m+[m[32m *[m
[32m+[m[32m *  if these values are null, "inactive" is used[m
[32m+[m[32m *[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mliberator.plugins.imeController = (function(){[m
[32m+[m[32m    var inputElement = document.getAnonymousElementByAttribute([m
[32m+[m[32m        document.getElementById('liberator-commandline-command'),'anonid','input'[m
[32m+[m[32m        );[m[41m [m
[32m+[m[32m    function getMode(name){[m
[32m+[m[32m        return liberator.globalVariables[name] ? liberator.globalVariables[name] : 'inactive';[m
[32m+[m[32m    }[m[41m   [m
[32m+[m[32m    function preExec(target,name,func){[m
[32m+[m[32m        var original = target[name];[m
[32m+[m[32m        target[name] = function(){[m
[32m+[m[32m            func.apply(this,arguments);[m
[32m+[m[32m            return original.apply(target,arguments);[m
[32m+[m[32m        }[m[41m   [m
[32m+[m[32m    }[m[41m   [m
[32m+[m[32m    preExec(commandline,'open',function(){[m
[32m+[m[32m        liberator.plugins.imeController.set(inputElement, getMode('ex_ime_mode'));[m
[32m+[m[32m    });[m
[32m+[m[32m    preExec(events,'onEscape',function(){[m
[32m+[m[32m        if (liberator.mode == modes.INSERT && (modes.extended & modes.TEXTAREA) && !options.insertmode){[m
[32m+[m[32m            var inputField = buffer.lastInputField;[m
[32m+[m[32m            if (liberator.plugins.imeController.set(inputField, getMode('textarea_ime_mode'))){[m
[32m+[m[32m                inputField.blur();[m
[32m+[m[32m                setTimeout(function(){inputField.focus();},0);[m
[32m+[m[32m            }[m[41m   [m
[32m+[m[32m        }[m[41m   [m
[32m+[m[32m    });[m
[32m+[m[32m    return {[m
[32m+[m[32m        set: function(elem, mode){[m
[32m+[m[32m            if (elem && mode) return elem.style.imeMode = mode;[m
[32m+[m[32m            return false;[m
[32m+[m[32m        },[m[41m [m
[32m+[m[32m            reset: function(){[m
[32m+[m[32m                delete liberator.globalVariables.ex_ime_mode;[m
[32m+[m[32m                delete liberator.globalVariables.textarea_ime_mode;[m
[32m+[m[32m            }[m[41m   [m
[32m+[m[32m    };[m[41m [m
[32m+[m[32m})();[m
[32m+[m[32m// vim: sw=4 ts=4 et:[m
[1mdiff --git a/rc.lua b/rc.lua[m
[1mnew file mode 100644[m
[1mindex 0000000..46d4eb4[m
[1m--- /dev/null[m
[1m+++ b/rc.lua[m
[36m@@ -0,0 +1,462 @@[m
[32m+[m[32m-- Standard awesome library[m
[32m+[m[32mlocal gears = require("gears")[m
[32m+[m[32mlocal awful = require("awful")[m
[32m+[m[32mawful.rules = require("awful.rules")[m
[32m+[m[32mrequire("awful.autofocus")[m
[32m+[m[32m-- Widget and layout library[m
[32m+[m[32mlocal wibox = require("wibox")[m
[32m+[m[32m-- Theme handling library[m
[32m+[m[32mlocal beautiful = require("beautiful")[m
[32m+[m[32m-- Notification library[m
[32m+[m[32mlocal naughty = require("naughty")[m
[32m+[m[32mlocal menubar = require("menubar")[m
[32m+[m
[32m+[m
[32m+[m[32m-- {{{ Error handling[m
[32m+[m[32m-- Check if awesome encountered an error during startup and fell back to[m
[32m+[m[32m-- another config (This code will only ever execute for the fallback config)[m
[32m+[m[32mif awesome.startup_errors then[m
[32m+[m[32m    naughty.notify({ preset = naughty.config.presets.critical,[m
[32m+[m[32m                     title = "Oops, there were errors during startup!",[m
[32m+[m[32m                     text = awesome.startup_errors })[m
[32m+[m[32mend[m
[32m+[m
[32m+[m[32m-- Handle runtime errors after startup[m
[32m+[m[32mdo[m
[32m+[m[32m    local in_error = false[m
[32m+[m[32m    awesome.connect_signal("debug::error", function (err)[m
[32m+[m[32m        -- Make sure we don't go into an endless error loop[m
[32m+[m[32m        if in_error then return end[m
[32m+[m[32m        in_error = true[m
[32m+[m
[32m+[m[32m        naughty.notify({ preset = naughty.config.presets.critical,[m
[32m+[m[32m                         title = "Oops, an error happened!",[m
[32m+[m[32m                         text = err })[m
[32m+[m[32m        in_error = false[m
[32m+[m[32m    end)[m
[32m+[m[32mend[m
[32m+[m[32m-- }}}[m
[32m+[m
[32m+[m[32m-- {{{ Variable definitions[m
[32m+[m[32m-- Themes define colours, icons, font and wallpapers.[m
[32m+[m[32m-- theme_name = "elzup" -- extends elric theme[m
[32m+[m[32m-- theme_name = "elric"[m
[32m+[m[32m-- theme_name = "blackburn"[m
[32m+[m[32mtheme_name = "blue"[m
[32m+[m[32m-- theme_name = "cool-blue"[m
[32m+[m[32mbeautiful.init("~/.config/awesome/themes/" .. theme_name .. "/theme.lua")[m
[32m+[m
[32m+[m[32m-- This is used later as the default terminal and editor to run.[m
[32m+[m[32mterminal = "xterm"[m
[32m+[m[32meditor = os.getenv("EDITOR") or "nano"[m
[32m+[m[32meditor_cmd = terminal .. " -e " .. editor[m
[32m+[m
[32m+[m[32m-- Default modkey.[m
[32m+[m[32m-- Usually, Mod4 is the key with a logo between Control and Alt.[m
[32m+[m[32m-- If you do not like this or do not have such a key,[m
[32m+[m[32m-- I suggest you to remap Mod4 to another key using xmodmap or other tools.[m
[32m+[m[32m-- However, you can use another modifier like Mod1, but it may interact with others.[m
[32m+[m[32mmodkey = "Mod4"[m
[32m+[m
[32m+[m[32m-- Table of layouts to cover with awful.layout.inc, order matters.[m
[32m+[m[32mlocal layouts =[m
[32m+[m[32m{[m
[32m+[m[32m    awful.layout.suit.floating,[m
[32m+[m[32m    awful.layout.suit.tile,[m
[32m+[m[32m    awful.layout.suit.tile.left,[m
[32m+[m[32m    awful.layout.suit.tile.bottom,[m
[32m+[m[32m    awful.layout.suit.tile.top,[m
[32m+[m[32m    awful.layout.suit.fair,[m
[32m+[m[32m    awful.layout.suit.fair.horizontal,[m
[32m+[m[32m    awful.layout.suit.spiral,[m
[32m+[m[32m    awful.layout.suit.spiral.dwindle,[m
[32m+[m[32m    awful.layout.suit.max,[m
[32m+[m[32m    awful.layout.suit.max.fullscreen,[m
[32m+[m[32m    awful.layout.suit.magnifier[m
[32m+[m[32m}[m
[32m+[m[32m-- }}}[m
[32m+[m
[32m+[m[32m-- {{{ Wallpaper[m
[32m+[m[32m-- if theme_name == "elzup" then[m
[32m+[m[32m--     while true[m
[32m+[m[32m--         do[m
[32m+[m[32m--             gears.wallpaper.maximized(beautiful.wallpaper, s, true)[m
[32m+[m[32m--             sleep 15m[m
[32m+[m[32m--     done &[m
[32m+[m[32m-- elseif beautiful.wallpaper then[m
[32m+[m[32mif beautiful.wallpaper then[m
[32m+[m[32m    for s = 1, screen.count() do[m
[32m+[m[32m        gears.wallpaper.maximized(beautiful.wallpaper, s, true)[m
[32m+[m[32m    end[m
[32m+[m[32mend[m
[32m+[m[32m-- }}}[m
[32m+[m
[32m+[m[32m-- {{{ Tags[m
[32m+[m[32m-- Define a tag table which hold all screen tags.[m
[32m+[m[32mtags = {}[m
[32m+[m[32mfor s = 1, screen.count() do[m
[32m+[m[32m    -- Each screen has its own tag table.[m
[32m+[m[32m    tags[s] = awful.tag({ 1, 2, 3, 4, 5, 6, 7, 8, 9 }, s, layouts[1])[m
[32m+[m[32mend[m
[32m+[m[32m-- }}}[m
[32m+[m
[32m+[m[32m-- {{{ Menu[m
[32m+[m[32m-- Create a laucher widget and a main menu[m
[32m+[m[32mmyawesomemenu = {[m
[32m+[m[32m   { "manual", terminal .. " -e man awesome" },[m
[32m+[m[32m   { "edit config", editor_cmd .. " " .. awesome.conffile },[m
[32m+[m[32m   { "restart", awesome.restart },[m
[32m+[m[32m   { "quit", awesome.quit }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mmymainmenu = awful.menu({ items = { { "awesome", myawesomemenu, beautiful.awesome_icon },[m
[32m+[m[32m                                    { "open terminal", terminal }[m
[32m+[m[32m                                  }[m
[32m+[m[32m                        })[m
[32m+[m
[32m+[m[32mmylauncher = awful.widget.launcher({ image = beautiful.awesome_icon,[m
[32m+[m[32m                                     menu = mymainmenu })[m
[32m+[m
[32m+[m[32m-- Menubar configuration[m
[32m+[m[32mmenubar.utils.terminal = terminal -- Set the terminal for applications that require it[m
[32m+[m[32mmenubar.cache_entries = true[m
[32m+[m[32mmenubar.app_folders = { "/usr/share/applications/", "~/.gnome/apps/" }[m
[32m+[m[32m-- }}}[m
[32m+[m
[32m+[m[32m-- {{{ Wibox[m
[32m+[m[32m-- Create a textclock widget[m
[32m+[m[32mmytextclock = awful.widget.textclock()[m
[32m+[m[32m-- mytextclock = awful.widget.textclock({ align = "right" }, "%m/%d %H:%%5M:%S")[m
[32m+[m[32m-- mytextclock = awful.widget.textclock({ align = "right" }, " %Y-%m-%d %a %H:%M:%S ", 1)[m
[32m+[m
[32m+[m[32m-- Create a wibox for each screen and add it[m
[32m+[m[32mmywibox = {}[m
[32m+[m[32mmypromptbox = {}[m
[32m+[m[32mmylayoutbox = {}[m
[32m+[m[32mmytaglist = {}[m
[32m+[m[32mmytaglist.buttons = awful.util.table.join([m
[32m+[m[32m                    awful.button({ }, 1, awful.tag.viewonly),[m
[32m+[m[32m                    awful.button({ modkey }, 1, awful.client.movetotag),[m
[32m+[m[32m                    awful.button({ }, 3, awful.tag.viewtoggle),[m
[32m+[m[32m                    awful.button({ modkey }, 3, awful.client.toggletag),[m
[32m+[m[32m                    awful.button({ }, 4, function(t) awful.tag.viewnext(awful.tag.getscreen(t)) end),[m
[32m+[m[32m                    awful.button({ }, 5, function(t) awful.tag.viewprev(awful.tag.getscreen(t)) end)[m
[32m+[m[32m                    )[m
[32m+[m[32mmytasklist = {}[m
[32m+[m[32mmytasklist.buttons = awful.util.table.join([m
[32m+[m[32m                     awful.button({ }, 1, function (c)[m
[32m+[m[32m                                              if c == client.focus then[m
[32m+[m[32m                                                  c.minimized = true[m
[32m+[m[32m                                              else[m
[32m+[m[32m                                                  -- Without this, the following[m
[32m+[m[32m                                                  -- :isvisible() makes no sense[m
[32m+[m[32m                                                  c.minimized = false[m
[32m+[m[32m                                                  if not c:isvisible() then[m
[32m+[m[32m                                                      awful.tag.viewonly(c:tags()[1])[m
[32m+[m[32m                                                  end[m
[32m+[m[32m                                                  -- This will also un-minimize[m
[32m+[m[32m                                                  -- the client, if needed[m
[32m+[m[32m                                                  client.focus = c[m
[32m+[m[32m                                                  c:raise()[m
[32m+[m[32m                                              end[m
[32m+[m[32m                                          end),[m
[32m+[m[32m                     awful.button({ }, 3, function ()[m
[32m+[m[32m                                              if instance then[m
[32m+[m[32m                                                  instance:hide()[m
[32m+[m[32m                                                  instance = nil[m
[32m+[m[32m                                              else[m
[32m+[m[32m                                                  instance = awful.menu.clients({[m
[32m+[m[32m                                                      theme = { width = 250 }[m
[32m+[m[32m                                                  })[m
[32m+[m[32m                                              end[m
[32m+[m[32m                                          end),[m
[32m+[m[32m                     awful.button({ }, 4, function ()[m
[32m+[m[32m                                              awful.client.focus.byidx(1)[m
[32m+[m[32m                                              if client.focus then client.focus:raise() end[m
[32m+[m[32m                                          end),[m
[32m+[m[32m                     awful.button({ }, 5, function ()[m
[32m+[m[32m                                              awful.client.focus.byidx(-1)[m
[32m+[m[32m                                              if client.focus then client.focus:raise() end[m
[32m+[m[32m                                          end))[m
[32m+[m
[32m+[m[32mfor s = 1, screen.count() do[m
[32m+[m[32m    -- Create a promptbox for each screen[m
[32m+[m[32m    mypromptbox[s] = awful.widget.prompt()[m
[32m+[m[32m    -- Create an imagebox widget which will contains an icon indicating which layout we're using.[m
[32m+[m[32m    -- We need one layoutbox per screen.[m
[32m+[m[32m    mylayoutbox[s] = awful.widget.layoutbox(s)[m
[32m+[m[32m    mylayoutbox[s]:buttons(awful.util.table.join([m
[32m+[m[32m                           awful.button({ }, 1, function () awful.layout.inc(layouts, 1) end),[m
[32m+[m[32m                           awful.button({ }, 3, function () awful.layout.inc(layouts, -1) end),[m
[32m+[m[32m                           awful.button({ }, 4, function () awful.layout.inc(layouts, 1) end),[m
[32m+[m[32m                           awful.button({ }, 5, function () awful.layout.inc(layouts, -1) end)))[m
[32m+[m[32m    -- Create a taglist widget[m
[32m+[m[32m    mytaglist[s] = awful.widget.taglist(s, awful.widget.taglist.filter.all, mytaglist.buttons)[m
[32m+[m
[32m+[m[32m    -- Create a tasklist widget[m
[32m+[m[32m    mytasklist[s] = awful.widget.tasklist(s, awful.widget.tasklist.filter.currenttags, mytasklist.buttons)[m
[32m+[m
[32m+[m[32m    -- Create the wibox[m
[32m+[m[32m    mywibox[s] = awful.wibox({ position = "top", screen = s })[m
[32m+[m
[32m+[m[32m    -- Widgets that are aligned to the left[m
[32m+[m[32m    local left_layout = wibox.layout.fixed.horizontal()[m
[32m+[m[32m    left_layout:add(mylauncher)[m
[32m+[m[32m    left_layout:add(mytaglist[s])[m
[32m+[m[32m    left_layout:add(mypromptbox[s])[m
[32m+[m
[32m+[m[32m    -- Widgets that are aligned to the right[m
[32m+[m[32m    local right_layout = wibox.layout.fixed.horizontal()[m
[32m+[m[32m    if s == 1 then right_layout:add(wibox.widget.systray()) end[m
[32m+[m[32m    right_layout:add(mytextclock)[m
[32m+[m[32m    right_layout:add(mylayoutbox[s])[m
[32m+[m
[32m+[m[32m    -- Now bring it all together (with the tasklist in the middle)[m
[32m+[m[32m    local layout = wibox.layout.align.horizontal()[m
[32m+[m[32m    layout:set_left(left_layout)[m
[32m+[m[32m    layout:set_middle(mytasklist[s])[m
[32m+[m[32m    layout:set_right(right_layout)[m
[32m+[m
[32m+[m[32m    mywibox[s]:set_widget(layout)[m
[32m+[m[32mend[m
[32m+[m[32m-- }}}[m
[32m+[m
[32m+[m[32m-- {{{ Mouse bindings[m
[32m+[m[32mroot.buttons(awful.util.table.join([m
[32m+[m[32m    awful.button({ }, 3, function () mymainmenu:toggle() end),[m
[32m+[m[32m    awful.button({ }, 4, awful.tag.viewnext),[m
[32m+[m[32m    awful.button({ }, 5, awful.tag.viewprev)[m
[32m+[m[32m))[m
[32m+[m[32m-- }}}[m
[32m+[m
[32m+[m[32m-- {{{ Key bindings[m
[32m+[m[32mglobalkeys = awful.util.table.join([m
[32m+[m[32m    awful.key({ modkey,           }, "h",   awful.tag.viewprev       ),[m
[32m+[m[32m    awful.key({ modkey,           }, "l",  awful.tag.viewnext       ),[m
[32m+[m[32m    awful.key({ modkey,           }, "Escape", awful.tag.history.restore),[m
[32m+[m
[32m+[m[32m    awful.key({ modkey,           }, "j",[m
[32m+[m[32m        function ()[m
[32m+[m[32m            awful.client.focus.byidx( 1)[m
[32m+[m[32m            if client.focus then client.focus:raise() end[m
[32m+[m[32m        end),[m
[32m+[m[32m    awful.key({ modkey,           }, "k",[m
[32m+[m[32m        function ()[m
[32m+[m[32m            awful.client.focus.byidx(-1)[m
[32m+[m[32m            if client.focus then client.focus:raise() end[m
[32m+[m[32m        end),[m
[32m+[m[32m    awful.key({ modkey,           }, "w", function () mymainmenu:show() end),[m
[32m+[m
[32m+[m[32m    -- Layout manipulation[m
[32m+[m[32m    awful.key({ modkey, "Shift"   }, "j", function () awful.client.swap.byidx(  1)    end),[m
[32m+[m[32m    awful.key({ modkey, "Shift"   }, "k", function () awful.client.swap.byidx( -1)    end),[m
[32m+[m[32m    awful.key({ modkey, "Control" }, "j", function () awful.screen.focus_relative( 1) end),[m
[32m+[m[32m    awful.key({ modkey, "Control" }, "k", function () awful.screen.focus_relative(-1) end),[m
[32m+[m[32m    awful.key({ modkey,           }, "u", awful.client.urgent.jumpto),[m
[32m+[m[32m    awful.key({ modkey,           }, "Tab",[m
[32m+[m[32m        function ()[m
[32m+[m[32m            awful.client.focus.history.previous()[m
[32m+[m[32m            if client.focus then[m
[32m+[m[32m                client.focus:raise()[m
[32m+[m[32m            end[m
[32m+[m[32m        end),[m
[32m+[m
[32m+[m[32m    -- Standard program[m
[32m+[m[32m    awful.key({ modkey,           }, "Return", function () awful.util.spawn(terminal) end),[m
[32m+[m[32m    awful.key({ modkey, "Control" }, "r", awesome.restart),[m
[32m+[m[32m    awful.key({ modkey, "Shift"   }, "q", awesome.quit),[m
[32m+[m
[32m+[m[32m    awful.key({ modkey,           }, "Left",     function () awful.tag.incmwfact( 0.05)    end),[m
[32m+[m[32m    awful.key({ modkey,           }, "Right",     function () awful.tag.incmwfact(-0.05)    end),[m
[32m+[m[32m    awful.key({ modkey, "Shift"   }, "h",     function () awful.tag.incnmaster( 1)      end),[m
[32m+[m[32m    awful.key({ modkey, "Shift"   }, "l",     function () awful.tag.incnmaster(-1)      end),[m
[32m+[m[32m    awful.key({ modkey, "Control" }, "h",     function () awful.tag.incncol( 1)         end),[m
[32m+[m[32m    awful.key({ modkey, "Control" }, "l",     function () awful.tag.incncol(-1)         end),[m
[32m+[m[32m    awful.key({ modkey,           }, "space", function () awful.layout.inc(layouts,  1) end),[m
[32m+[m[32m    awful.key({ modkey, "Shift"   }, "space", function () awful.layout.inc(layouts, -1) end),[m
[32m+[m
[32m+[m[32m    awful.key({ modkey, "Control" }, "n", awful.client.restore),[m
[32m+[m
[32m+[m[32m    -- Prompt[m
[32m+[m[32m    awful.key({ modkey },            "r",     function () mypromptbox[mouse.screen]:run() end),[m
[32m+[m
[32m+[m[32m    awful.key({ modkey }, "x",[m
[32m+[m[32m              function ()[m
[32m+[m[32m                  awful.prompt.run({ prompt = "Run Lua code: " },[m
[32m+[m[32m                  mypromptbox[mouse.screen].widget,[m
[32m+[m[32m                  awful.util.eval, nil,[m
[32m+[m[32m                  awful.util.getdir("cache") .. "/history_eval")[m
[32m+[m[32m              end),[m
[32m+[m[32m    -- Menubar[m
[32m+[m[32m    awful.key({ modkey }, "p", function() menubar.show() end)[m
[32m+[m[32m)[m
[32m+[m
[32m+[m[32mclientkeys = awful.util.table.join([m
[32m+[m[32m    awful.key({ modkey,           }, "f",      function (c) c.fullscreen = not c.fullscreen  end),[m
[32m+[m[32m    awful.key({ modkey, "Shift"   }, "c",      function (c) c:kill()                         end),[m
[32m+[m[32m    awful.key({ modkey, "Control" }, "space",  awful.client.floating.toggle                     ),[m
[32m+[m[32m    awful.key({ modkey, "Control" }, "Return", function (c) c:swap(awful.client.getmaster()) end),[m
[32m+[m[32m    awful.key({ modkey,           }, "o",      awful.client.movetoscreen                        ),[m
[32m+[m[32m    awful.key({ modkey,           }, "t",      function (c) c.ontop = not c.ontop            end),[m
[32m+[m[32m    awful.key({ modkey,           }, "s",[m
[32m+[m[32m        function (c)[m
[32m+[m[32m            -- The client currently has the input focus, so it cannot be[m
[32m+[m[32m            -- minimized, since minimized clients can't have the focus.[m
[32m+[m[32m            c.minimized = true[m
[32m+[m[32m        end),[m
[32m+[m[32m    awful.key({ modkey,           }, "m",[m
[32m+[m[32m        function (c)[m
[32m+[m[32m            c.maximized_horizontal = not c.maximized_horizontal[m
[32m+[m[32m            c.maximized_vertical   = not c.maximized_vertical[m
[32m+[m[32m        end)[m
[32m+[m[32m)[m
[32m+[m
[32m+[m[32m-- Bind all key numbers to tags.[m
[32m+[m[32m-- Be careful: we use keycodes to make it works on any keyboard layout.[m
[32m+[m[32m-- This should map on the top row of your keyboard, usually 1 to 9.[m
[32m+[m[32mfor i = 1, 9 do[m
[32m+[m[32m    globalkeys = awful.util.table.join(globalkeys,[m
[32m+[m[32m        -- View tag only.[m
[32m+[m[32m        awful.key({ modkey }, "#" .. i + 9,[m
[32m+[m[32m                  function ()[m
[32m+[m[32m                        local screen = mouse.screen[m
[32m+[m[32m                        local tag = awful.tag.gettags(screen)[i][m
[32m+[m[32m                        if tag then[m
[32m+[m[32m                           awful.tag.viewonly(tag)[m
[32m+[m[32m                        end[m
[32m+[m[32m                  end),[m
[32m+[m[32m        -- Toggle tag.[m
[32m+[m[32m        awful.key({ modkey, "Control" }, "#" .. i + 9,[m
[32m+[m[32m                  function ()[m
[32m+[m[32m                      local screen = mouse.screen[m
[32m+[m[32m                      local tag = awful.tag.gettags(screen)[i][m
[32m+[m[32m                      if tag then[m
[32m+[m[32m                         awful.tag.viewtoggle(tag)[m
[32m+[m[32m                      end[m
[32m+[m[32m                  end),[m
[32m+[m[32m        -- Move client to tag.[m
[32m+[m[32m        awful.key({ modkey, "Shift" }, "#" .. i + 9,[m
[32m+[m[32m                  function ()[m
[32m+[m[32m                      if client.focus then[m
[32m+[m[32m                          local tag = awful.tag.gettags(client.focus.screen)[i][m
[32m+[m[32m                          if tag then[m
[32m+[m[32m                              awful.client.movetotag(tag)[m
[32m+[m[32m                          end[m
[32m+[m[32m                     end[m
[32m+[m[32m                  end),[m
[32m+[m[32m        -- Toggle tag.[m
[32m+[m[32m        awful.key({ modkey, "Control", "Shift" }, "#" .. i + 9,[m
[32m+[m[32m                  function ()[m
[32m+[m[32m                      if client.focus then[m
[32m+[m[32m                          local tag = awful.tag.gettags(client.focus.screen)[i][m
[32m+[m[32m                          if tag then[m
[32m+[m[32m                              awful.client.toggletag(tag)[m
[32m+[m[32m                          end[m
[32m+[m[32m                      end[m
[32m+[m[32m                  end))[m
[32m+[m[32mend[m
[32m+[m
[32m+[m[32mclientbuttons = awful.util.table.join([m
[32m+[m[32m    awful.button({ }, 1, function (c) client.focus = c; c:raise() end),[m
[32m+[m[32m    awful.button({ modkey }, 1, awful.mouse.client.move),[m
[32m+[m[32m    awful.button({ modkey }, 3, awful.mouse.client.resize))[m
[32m+[m
[32m+[m[32m-- Set keys[m
[32m+[m[32mroot.keys(globalkeys)[m
[32m+[m[32m-- }}}[m
[32m+[m
[32m+[m[32m-- {{{ Rules[m
[32m+[m[32m-- Rules to apply to new clients (through the "manage" signal).[m
[32m+[m[32mawful.rules.rules = {[m
[32m+[m[32m    -- All clients will match this rule.[m
[32m+[m[32m    { rule = { },[m
[32m+[m[32m      properties = { border_width = beautiful.border_width,[m
[32m+[m[32m                     border_color = beautiful.border_normal,[m
[32m+[m[32m                     focus = awful.client.focus.filter,[m
[32m+[m[32m                     raise = true,[m
[32m+[m[32m                     keys = clientkeys,[m
[32m+[m[32m                     buttons = clientbuttons } },[m
[32m+[m[32m    { rule = { class = "MPlayer" },[m
[32m+[m[32m      properties = { floating = true } },[m
[32m+[m[32m    { rule = { class = "pinentry" },[m
[32m+[m[32m      properties = { floating = true } },[m
[32m+[m[32m    { rule = { class = "gimp" },[m
[32m+[m[32m      properties = { floating = true } },[m
[32m+[m[32m    -- Set Firefox to always map on tags number 2 of screen 1.[m
[32m+[m[32m    -- { rule = { class = "Firefox" },[m
[32m+[m[32m    --   properties = { tag = tags[1][2] } },[m
[32m+[m[32m}[m
[32m+[m[32m-- }}}[m
[32m+[m
[32m+[m[32m-- {{{ Signals[m
[32m+[m[32m-- Signal function to execute when a new client appears.[m
[32m+[m[32mclient.connect_signal("manage", function (c, startup)[m
[32m+[m[32m    -- Enable sloppy focus[m
[32m+[m[32m    c:connect_signal("mouse::enter", function(c)[m
[32m+[m[32m        if awful.layout.get(c.screen) ~= awful.layout.suit.magnifier[m
[32m+[m[32m            and awful.client.focus.filter(c) then[m
[32m+[m[32m            client.focus = c[m
[32m+[m[32m        end[m
[32m+[m[32m    end)[m
[32m+[m
[32m+[m[32m    if not startup then[m
[32m+[m[32m        -- Set the windows at the slave,[m
[32m+[m[32m        -- i.e. put it at the end of others instead of setting it master.[m
[32m+[m[32m        -- awful.client.setslave(c)[m
[32m+[m
[32m+[m[32m        -- Put windows in a smart way, only if they does not set an initial position.[m
[32m+[m[32m        if not c.size_hints.user_position and not c.size_hints.program_position then[m
[32m+[m[32m            awful.placement.no_overlap(c)[m
[32m+[m[32m            awful.placement.no_offscreen(c)[m
[32m+[m[32m        end[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    local titlebars_enabled = false[m
[32m+[m[32m    if titlebars_enabled and (c.type == "normal" or c.type == "dialog") then[m
[32m+[m[32m        -- buttons for the titlebar[m
[32m+[m[32m        local buttons = awful.util.table.join([m
[32m+[m[32m                awful.button({ }, 1, function()[m
[32m+[m[32m                    client.focus = c[m
[32m+[m[32m                    c:raise()[m
[32m+[m[32m                    awful.mouse.client.move(c)[m
[32m+[m[32m                end),[m
[32m+[m[32m                awful.button({ }, 3, function()[m
[32m+[m[32m                    client.focus = c[m
[32m+[m[32m                    c:raise()[m
[32m+[m[32m                    awful.mouse.client.resize(c)[m
[32m+[m[32m                end)[m
[32m+[m[32m                )[m
[32m+[m
[32m+[m[32m        -- Widgets that are aligned to the left[m
[32m+[m[32m        local left_layout = wibox.layout.fixed.horizontal()[m
[32m+[m[32m        left_layout:add(awful.titlebar.widget.iconwidget(c))[m
[32m+[m[32m        left_layout:buttons(buttons)[m
[32m+[m
[32m+[m[32m        -- Widgets that are aligned to the right[m
[32m+[m[32m        local right_layout = wibox.layout.fixed.horizontal()[m
[32m+[m[32m        right_layout:add(awful.titlebar.widget.floatingbutton(c))[m
[32m+[m[32m        right_layout:add(awful.titlebar.widget.maximizedbutton(c))[m
[32m+[m[32m        right_layout:add(awful.titlebar.widget.stickybutton(c))[m
[32m+[m[32m        right_layout:add(awful.titlebar.widget.ontopbutton(c))[m
[32m+[m[32m        right_layout:add(awful.titlebar.widget.closebutton(c))[m
[32m+[m
[32m+[m[32m        -- The title goes in the middle[m
[32m+[m[32m        local middle_layout = wibox.layout.flex.horizontal()[m
[32m+[m[32m        local title = awful.titlebar.widget.titlewidget(c)[m
[32m+[m[32m        title:set_align("center")[m
[32m+[m[32m        middle_layout:add(title)[m
[32m+[m[32m        middle_layout:buttons(buttons)[m
[32m+[m
[32m+[m[32m        -- Now bring it all together[m
[32m+[m[32m        local layout = wibox.layout.align.horizontal()[m
[32m+[m[32m        layout:set_left(left_layout)[m
[32m+[m[32m        layout:set_right(right_layout)[m
[32m+[m[32m        layout:set_middle(middle_layout)[m
[32m+[m
[32m+[m[32m        awful.titlebar(c):set_widget(layout)[m
[32m+[m[32m    end[m
[32m+[m[32mend)[m
[32m+[m
[32m+[m[32m-- client.connect_signal("focus", function(c) c.border_color = beautiful.border_focus end)[m
[32m+[m[32m-- client.connect_signal("unfocus", function(c) c.border_color = beautiful.border_normal end)[m
[32m+[m[32m-- }}}[m
