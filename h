[32m+[m[32m    let s:enc_euc = 'eucjp-ms'[m
[32m+[m[32m    let s:enc_jis = 'iso-2022-jp-3'[m
[32m+[m[32m  " iconv„ÅåJISX0213„Å´ÂØæÂøú„Åó„Å¶„ÅÑ„Çã„Åã„Çí„ÉÅ„Çß„ÉÉ„ÇØ[m
[32m+[m[32m  elseif iconv("\x87\x64\x87\x6a", 'cp932', 'euc-jisx0213') ==# "\xad\xc5\xad\xcb"[m
[32m+[m[32m    let s:enc_euc = 'euc-jisx0213'[m
[32m+[m[32m    let s:enc_jis = 'iso-2022-jp-3'[m
[32m+[m[32m  endif[m
[32m+[m[32m  " fileencodings„ÇíÊßãÁØâ[m
[32m+[m[32m  if &encoding ==# 'utf-8'[m
[32m+[m[32m    let s:fileencodings_default = &fileencodings[m
[32m+[m[32m    let &fileencodings = s:enc_jis .','. s:enc_euc .',cp932'[m
[32m+[m[32m    let &fileencodings = &fileencodings .','. s:fileencodings_default[m
[32m+[m[32m    unlet s:fileencodings_default[m
[32m+[m[32m  else[m
[32m+[m[32m    let &fileencodings = &fileencodings .','. s:enc_jis[m
[32m+[m[32m    set fileencodings+=utf-8,ucs-2le,ucs-2[m
[32m+[m[32m    if &encoding =~# '^\(euc-jp\|euc-jisx0213\|eucjp-ms\)$'[m
[32m+[m[32m      set fileencodings+=cp932[m
[32m+[m[32m      set fileencodings-=euc-jp[m
[32m+[m[32m      set fileencodings-=euc-jisx0213[m
[32m+[m[32m      set fileencodings-=eucjp-ms[m
[32m+[m[32m      let &encoding = s:enc_euc[m
[32m+[m[32m      let &fileencoding = s:enc_euc[m
[32m+[m[32m    else[m
[32m+[m[32m      let &fileencodings = &fileencodings .','. s:enc_euc[m
[32m+[m[32m    endif[m
[32m+[m[32m  endif[m
[32m+[m[32m  " ÂÆöÊï∞„ÇíÂá¶ÂàÜ[m
[32m+[m[32m  unlet s:enc_euc[m
[32m+[m[32m  unlet s:enc_jis[m
[32m+[m[32mendif[m
[32m+[m[32m" Êó•Êú¨Ë™û„ÇíÂê´„Åæ„Å™„ÅÑÂ†¥Âêà„ÅØ fileencoding „Å´ encoding „Çí‰Ωø„ÅÜ„Çà„ÅÜ„Å´„Åô„Çã[m
[32m+[m[32mif has('autocmd')[m
[32m+[m[32m  function! AU_ReCheck_FENC()[m
[32m+[m[32m    if &fileencoding =~# 'iso-2022-jp' && search("[^\x01-\x7e]", 'n') == 0[m
[32m+[m[32m      let &fileencoding=&encoding[m
[32m+[m[32m    endif[m
[32m+[m[32m  endfunction[m
[32m+[m[32m  autocmd BufReadPost * call AU_ReCheck_FENC()[m
[32m+[m[32mendif[m
[32m+[m[32m" ÊîπË°å„Ç≥„Éº„Éâ„ÅÆËá™ÂãïË™çË≠ò[m
[32m+[m[32mset fileformats=unix,dos,mac[m
[32m+[m[32m" ‚ñ°„Å®„Åã‚óã„ÅÆÊñáÂ≠ó„Åå„ÅÇ„Å£„Å¶„ÇÇ„Ç´„Éº„ÇΩ„É´‰ΩçÁΩÆ„Åå„Åö„Çå„Å™„ÅÑ„Çà„ÅÜ„Å´„Åô„Çã[m
[32m+[m[32mif exists('&ambiwidth')[m
[32m+[m[32m  set ambiwidth=double[m
[32m+[m[32mendif[m
[32m+[m
[32m+[m[32m"  }}} -end -Encoding[m[41m [m
