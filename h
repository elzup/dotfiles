[32m+[m[32m"„Éê„Ç§„Éä„É™Á∑®ÈõÜ(xxd)„É¢„Éº„ÉâÔºàvim -b „Åß„ÅÆËµ∑Âãï„ÄÅ„ÇÇ„Åó„Åè„ÅØ *.bin „ÅßÁô∫Âãï„Åó„Åæ„ÅôÔºâ[m
[32m+[m[32maugroup BinaryXXD[m
[32m+[m[32m        autocmd![m
[32m+[m[32m        autocmd BufReadPre  *.bin let &binary =1[m
[32m+[m[32m        autocmd BufReadPost * if &binary | silent %!xxd -g 1[m
[32m+[m[32m        autocmd BufReadPost * set ft=xxd | endif[m
[32m+[m[32m        autocmd BufWritePre * if &binary | %!xxd -r[m
[32m+[m[32m        autocmd BufWritePre * endif[m
[32m+[m[32m        autocmd BufWritePost * if &binary | silent %!xxd -g 1[m
[32m+[m[32m        autocmd BufWritePost * set nomod | endif[m
[32m+[m[32maugroup END[m
[32m+[m
[32m+[m
[32m+[m[32m" -end}}}[m
[32m+[m[32m" auto position {{{ let g:save_window_file = expand('~/.vimwinpos')[m
[32m+[m[32maugroup SaveWindow[m
[32m+[m[32m  autocmd![m
[32m+[m[32m  autocmd VimLeavePre * call s:save_window()[m
[32m+[m[32m  function! s:save_window()[m
[32m+[m[32m    let options = [[m
[32m+[m[32m      \ 'set columns=' . &columns,[m
[32m+[m[32m      \ 'set lines=' . &lines,[m
[32m+[m[32m      \ 'winpos ' . getwinposx() . ' ' . getwinposy(),[m
[32m+[m[32m      \ ][m
[32m+[m[32m    call writefile(options, g:save_window_file)[m
[32m+[m[32m  endfunction[m
[32m+[m[32maugroup END[m
[32m+[m
[32m+[m[32m"if filereadable(g:save_window_file)[m
[32m+[m[32m"  execute 'source' g:save_window_file[m
[32m+[m[32m"endif[m
[32m+[m[32m"}}}[m
[32m+[m[32m" sound coding {{{[m
[32m+[m
[32m+[m[32m"autocmd CursorMovedI * :call vimproc#system_bg("~/.vim/sound/vim-key-sound.rb '" . getline('.')[col('.') - 2] . "'")[m
[32m+[m
[32m+[m[32m"" ÂäπÊûúÈü≥„Éá„Ç£„É¨„ÇØ„Éà„É™„Å®Êã°ÂºµÂ≠ê„ÅÆÊåáÂÆö„ÄÇÂäπÊûúÈü≥Âêç„ÇíÂÖÉ„Å´„Éï„É´„Éë„Çπ„Å´Â§âÊèõÂá∫Êù•„Çã„Çà„ÅÜ„Å´„ÄÇ[m
[32m+[m[32m"let s:se_path = "~/.vim/sound/type/"[m
[32m+[m[32m"let s:se_ext = ".wav"[m
[32m+[m[32m"function! s:change_sound_name(base_name)[m
[32m+[m[32m"  return expand(s:se_path . a:base_name . s:se_ext)[m
[32m+[m[32m"endfunction[m[41m [m
[32m+[m[32m"[m
[32m+[m[32m"" sound#play_wav„ÅÆ„É©„ÉÉ„Éë[m
[32m+[m[32m"function! PlaySE(name)[m
[32m+[m[32m"  call sound#play_wav(s:change_sound_name(a:name))[m
[32m+[m[32m"endfunction[m
[32m+[m[32m"[m
[32m+[m[32m""function! PlaySE(name)[m
[32m+[m[32m""  silent! exec '!afplay ' . s:se_path . a:name . s:se_ext . ' &'[m
[32m+[m[32m""endfunction[m
[32m+[m[32m"[m
[32m+[m[32m"[m
[32m+[m[32m""" „ÉÜ„Çπ„ÉàÁî®„ÄÇÂäπÊûúÈü≥Âêç„ÅÆ‰∏ä„Åß<Leader><Leader>„ÇíÊäº„Åô„Å®ÂÜçÁîüÂá∫Êù•„Çã„Çà„ÅÜ„Å´[m
[32m+[m[32m""command! -nargs=1 Play :call PlaySE("<args>")[m
[32m+[m[32m""nmap <Leader><Leader> :<C-u>Play <C-r><C-w><CR>[m
[32m+[m[32m"[m
[32m+[m[32m"" Ë£úÂÆå„Çí„ÅÆÂÆå‰∫Ü[m
